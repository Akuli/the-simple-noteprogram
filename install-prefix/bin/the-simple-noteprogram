#!/usr/bin/env python3
import sys
if sys.version_info[:2] < (3, 2):                           # NOQA
    sys.exit("This program requires Python 3.2 or newer.")  # NOQA
import importlib
from os.path import abspath, dirname, join

sys.path.append(join(dirname(dirname(abspath(__file__))), 'lib'))
main = importlib.import_module('the-simple-noteprogram.__main__').main

if __name__ == '__main__':
    main()
